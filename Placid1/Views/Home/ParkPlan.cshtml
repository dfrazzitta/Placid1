            <p>
                <img src="~/Images/Bullet.png" alt="Bullet" border="0" height="9" width="0" style="padding-right:4px;">
                <a href="@Url.Action("Location", "home")" class="menubar">Location</a>
            </p>
            <p>
                <img src="~/Images/Bullet.png" alt="Bullet" border="0" height="9" width="0" style="padding-right:5px;">
